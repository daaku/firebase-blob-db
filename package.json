{
  "name": "@daaku/firebase-blob-db",
  "author": "Naitik Shah <n@daaku.org>",
  "version": "2.1.0",
  "description": "Resilient uploading of blobs to Firebase Storage.",
  "repository": "git@github.com:daaku/firebase-blob-db",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "tsc",
    "test": "vite build && TIMEOUT=30000 qunit-puppeteer",
    "deploy": "npm run test && npm publish --access=public",
    "prepare": "npm run build",
    "dev": "vite"
  },
  "keywords": [
    "firebase",
    "api"
  ],
  "devDependencies": {
    "@daaku/firebase-auth": "^2.2.0",
    "@daaku/qunit-puppeteer": "^3.3.0",
    "@types/qunit": "^2.11.2",
    "nanoid": "^5.0.1",
    "qunit": "^2.17.2",
    "typescript": "^5.0.2",
    "vite": "^5.0.2"
  },
  "dependencies": {
    "@daaku/firebase-storage": "^2.0.0",
    "idb": "^7.0.2"
  }
}
